{
    "name": "@myorm/mysql",
    "version": "0.0.5",
    "homepage": "https://github.com/traviszuleger/myorm",
    "repository": {
        "type": "git",
        "url": "https://github.com/traviszuleger/myorm"
    },
    "description": "Provides an easy way to interact with the mysql2 connector by creating Contexts connected to a table in your MySQL database with only the need of defining your tables as TypeScript types.",
    "main": "./index.js",
    "types": "./index.js",
    "scripts": {
        "document": "jsdoc ./lib/src/ -t ./node_modules/docdash -c ./jsdoc-conf.json",
        "test": "node ./lib/tests/test.js"
    },
    "keywords": [
        "mysql",
        "context",
        "typescript",
        "C#",
        "table",
        "query",
        "queries",
        "insert",
        "update",
        "listener",
        "database",
        "get",
        "getAll",
        "insertOne",
        "insertMany",
        "updateOne",
        "updateMany",
        "deleteOne",
        "deleteMany",
        "truncate",
        "left join",
        "include",
        "relationships",
        "hasOne",
        "hasMany"
    ],
    "author": "Travis Zuleger",
    "license": "(MIT OR Apache-2.0)",
    "type": "module",
    "dependencies": {
        "mysql2": "^3.0.1"
    },
    "devDependencies": {
        "@types/node": "^18.11.18",
        "docdash": "^2.0.1",
        "typescript": "^4.9.4"
    },
    "files": [
        "lib/src/**/*"
    ],
    "exports": {
        ".": {
            "import": {
                "types": "./index.js",
                "default": "./index.js"
            },
            "require": {
                "types": "./index.js",
                "default": "./index.js"
            }
        }
    }
}
